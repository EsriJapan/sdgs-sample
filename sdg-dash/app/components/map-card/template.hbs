<div class="panel panel-default">
  <div class="panel-heading">
    <div class="pull-left">
      <span class="text-muted">{{model.component.settings.indicator_id}}</span>
      {{model.component.settings.title}}
    </div>
    <div class="pull-right">
      {{#if model.component.settings.data_info}}
        {{data-info-popover model=model.component.settings.data_info}}
      {{/if}}
    </div>
    <div class="clearfix"></div>
  </div>
  <div class="panel-body no-padding">
    {{arcgis-map webmap=model.component.settings.webmap settings=model.component.settings}}
  </div>
</div>